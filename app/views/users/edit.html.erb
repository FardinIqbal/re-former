<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username, :value => "Username" %>

  <%= f.label :email %>
  <%= f.text_field :email, :placeholder => "example@example.com" %>

  <%= f.label :password%>
  <%= f.password_field :password%>

  <%= f.submit "Submit" %>
<% end %>